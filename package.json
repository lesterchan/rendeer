{
  "name": "rendeer",
  "version": "1.2.0",
  "description": "When Puppeteer meets Prerender",
  "license": "MIT",
  "author": "Tech in Asia .develop <dev@techinasia.com> (https://www.techinasia.com)",
  "homepage": "https://github.com/techinasia/rendeer",
  "repository": {
    "type": "git",
    "url": "https://github.com/techinasia/rendeer.git"
  },
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint src/*.js",
    "start": "node src/main.js"
  },
  "eslintConfig": {
    "extends": "techinasia-base",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-unused-expressions": 0,
      "func-names": 0,
      "strict": 0,
      "max-len": 0,
      "no-console": 0,
      "no-param-reassign": 0
    }
  },
  "devDependencies": {
    "eslint": "*",
    "eslint-config-techinasia-base": "*",
    "eslint-plugin-import": "*"
  },
  "dependencies": {
    "md5": "~2.2.1",
    "memcache-plus": "~0.2.19",
    "p-timeout": "~2.0.1",
    "puppeteer": "~1.8.0"
  },
  "engines": {
    "node": ">=10.10"
  }
}
