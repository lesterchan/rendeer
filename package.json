{
  "name": "rendeer",
  "version": "1.5.0",
  "description": "When Puppeteer meets Prerender",
  "license": "MIT",
  "author": "Lester Chan <lesterchan@gmail.com> (https://lesterchan.net)",
  "homepage": "https://github.com/lesterchan/rendeer",
  "repository": {
    "type": "git",
    "url": "https://github.com/lesterchan/rendeer.git"
  },
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint src/*.js",
    "start": "node src/start.js"
  },
  "eslintConfig": {
    "extends": "techinasia-base",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-unused-expressions": 0,
      "func-names": 0,
      "strict": 0,
      "max-len": 0,
      "no-console": 0,
      "no-param-reassign": 0
    }
  },
  "devDependencies": {
    "eslint": "~5.9.0",
    "eslint-config-techinasia-base": "~0.8.0",
    "eslint-plugin-import": "~2.14.0"
  },
  "dependencies": {
    "md5": "~2.2.1",
    "memcache-plus": "~0.2.19",
    "p-timeout": "~2.0.1",
    "puppeteer": "~1.11.0"
  }
}
