{
  "name": "rendeer",
  "version": "1.5.0",
  "description": "When Puppeteer meets Prerender",
  "license": "MIT",
  "author": "Lester Chan <lesterchan@gmail.com> (https://lesterchan.net)",
  "homepage": "https://github.com/lesterchan/rendeer",
  "repository": {
    "type": "git",
    "url": "https://github.com/lesterchan/rendeer.git"
  },
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint src/*.js",
    "start": "node src/start.js",
    "test": "mocha"
  },
  "eslintConfig": {
    "extends": "techinasia-base",
    "env": {
      "browser": true,
      "mocha": true,
      "node": true
    },
    "rules": {
      "no-unused-expressions": 0,
      "func-names": 0,
      "strict": 0,
      "max-len": 0,
      "no-console": 0,
      "no-param-reassign": 0
    }
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.26.0",
    "eslint-config-techinasia-base": "^0.8.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.4.0"
  },
  "dependencies": {
    "md5": "^2.3.0",
    "memcache-plus": "^0.2.22",
    "p-timeout": "^4.1.0",
    "puppeteer": "^9.1.1"
  }
}
